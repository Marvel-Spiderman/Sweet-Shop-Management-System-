{
    "devCommand": "uvicorn main:app --port 8000",
    "builds": [
        {
            "src": "backend/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/docs",
            "dest": "backend/main.py"
        },
        {
            "src": "/openapi.json",
            "dest": "backend/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "backend/main.py"
        }
    ]
}